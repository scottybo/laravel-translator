{
    "name": "scottybo/laravel-translator",
    "description": "Fork of thiagocordeiro/laravel-translator to search translation keys in PHP and Vue/JS and insert into separate json files to be translated",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Bowler",
            "email": "scott.bowler@dcsworldwide.net"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Translator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Translator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Translator\\TranslatorServiceProvider"
            ]
        }
    }
}
